<idea-plugin>
    <id>org.github.ontanikotani.mathfolding</id>
    <name>Math folding</name>
    <version>1.0</version>
    <vendor url="https://github.com/Otanikotani/math-folding">
        Otanikotani
    </vendor>

    <idea-version since-build="193.5662"/>

    <description><![CDATA[
        Fold Math.abs, Math.pow, Math.sqrt, Math.cbrt into more readable, mathematical symbols.
        ]]></description>

    <extensions defaultExtensionNs="com.intellij">
        <lang.foldingBuilder language="JAVA" id="MathFoldingBuilder" implementationClass="org.github.otanikotani.mathfolding.MathFoldingBuilder"/>
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>

</idea-plugin>